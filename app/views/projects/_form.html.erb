<%= form_with model: @project, local: true, html: { enctype: "multipart/form-data" } do |form| %>

	<div class="form-group">
		<%= form.label :name %>
		<%= form.text_field :name, class: "form-control" %>
	</div>

	<div class="form-group">
		<%= form.label "Project Description" %>
		<%= form.text_area :description, class: "form-control" %>
	</div>

	<div class="form-group">
		<%= form.label :tech_stack %>
		<%= form.text_field :tech_stack, class: "form-control" %>
	</div>

	<div class="form-group">
		<%= form.label :deployment %>
		<%= form.text_field :deployment, class: "form-control" %>
	</div>

	<p>
		<%= form.submit 'Create Project', class: "btn btn-success" %>
	</p>

<% end %>
